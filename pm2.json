{
  "apps": [
    {
      "name": "django <port>",
      "script": "manage.py",
      "args": ["runserver", "0.0.0.0:<port>"],
      "interpreter": "python3",
      "instances": "1",
      "autorestart": true,
      "watch": true,
      "max_memory_restart": "1G",
      "env": {
        "DJANGO_SETTINGS_MODULE": "<carpeta>.settings"
      }
    }
  ]
}
